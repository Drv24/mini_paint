#:import C kivy.utils.get_color_from_hex

<RButton@Button>:
	font_size: 15
	bold: True
	background_normal: 'rbn.png'
	background_down: 'rbd.png'
	border: (2,10,2,10)

<ColorButton@RadioButton>:
	group: 'color'
	on_release: app.canvas_widget.set_color(self.background_color)
	background_normal: 'bn.png'
	background_down: 'bd.png'	
	border: (3,3,3,3)
	height: 40
	width: 50

<LineWidthButton@RadioButton>:
	group: 'line_width'
	on_release: app.canvas_widget.set_line_width(self.text)
	color: C('#2C3E50')
	background_color: C('#ECF0F1')
	background_normal: 'bn.png'
	background_down: 'bd.png'	
	border: (3,3,3,3)
	height: 40
	width: 30
	font_size: 15

<Canvas>:

	RButton:
		text: 'Change Background'
		right: root.right
		y: root.height - 30
		width: 150
		height: 30
		on_press: app.canvas_widget.set_bgcolor()

	RButton:
		id: b1
		text: 'Clear All'
		right: root.right
		y: root.height - 70
		width: 60
		height: 30
		on_press: root.clr()
	
	RButton:
		text: 'Eraser'
		right: root.right
		y: root.height - 110
		width: 60
		height: 30
		on_press: app.canvas_widget.set_color(app.canvas_widget.current_bgcolor)



